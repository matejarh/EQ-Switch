{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compile Resource",
            "type": "shell",
            "command": "rc",
            "args": [
                "/fo",
                "build/resource.res",
                "resources/resource.rc"
            ],
            "group": "build"
        },
        {
            "label": "Build EQ Switch (cl.exe)",
            "type": "shell",
            "command": "cl.exe",
            "args": [
                "/std:c++17",
                "/EHsc",
                "/DUNICODE",
                "/D_UNICODE",
                "/Fo:build\\",
                "/I",
                "imgui",
                "/I",
                "imgui\\backends",
                "/I",
                "src",
                "/I",
                "src\\gui",
                "/I",
                "src\\audio",
                "/I",
                "src\\utils",
                "/I",
                "src\\config",
                "/I",
                "resources",
                "imgui\\imgui.cpp",
                "imgui\\imgui_draw.cpp",
                "imgui\\imgui_tables.cpp",
                "imgui\\imgui_widgets.cpp",
                "imgui\\backends\\imgui_impl_win32.cpp",
                "imgui\\backends\\imgui_impl_dx11.cpp",
                "src\\utils\\kissfft\\kiss_fft.c",
                "src\\utils\\kissfft\\kiss_fftr.c",
                "src\\audio\\AudioCapture.cpp",
                "src\\audio\\VUBuffer.cpp",
                "src\\audio\\FrequencyVUMeter.cpp",
                "src\\utils\\AppLauncher.cpp",
                "src\\utils\\ChannelLabels.cpp",
                "src\\utils\\fonts.cpp",
                "src\\utils\\Theme.cpp",
                "src\\utils\\SystemUtils.cpp",
                "src\\utils\\EqualizerAPOManager.cpp",
                "src\\utils\\PopupUtils.cpp",
                "src\\utils\\ButtonUtils.cpp",
                "src\\config\\ProfileManager.cpp",
                "src\\gui\\EQSwitchWindow.cpp",
                "src\\gui\\_partials\\ProfilesSelector.cpp",
                "src\\gui\\_partials\\Footer.cpp",
                "src\\gui\\_partials\\VUMetersSection.cpp",
                "src\\gui\\_partials\\VUMeters.cpp",
                "src\\gui\\_partials\\CheckSection.cpp",
                "src\\main.cpp",
                "build\\resource.res",
                "/link",
                "/SUBSYSTEM:WINDOWS",
                "dxgi.lib",
                "d3d11.lib",
                "user32.lib",
                "gdi32.lib",
                "Shell32.lib",
                "ole32.lib",
                "uuid.lib",
                "Advapi32.lib",
                "/OUT:bin\\EQSwitch.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": [
                "$msCompile"
            ]
        },
        {
            "label": "Build Release",
            "type": "shell",
            "command": "cl.exe",
            "args": [
                "/std:c++17",
                "/EHsc",
                "/O2",
                "/MT",
                "/DNDEBUG",
                "/DUNICODE",
                "/D_UNICODE",
                "/Fo:build\\",
                "/I",
                "imgui",
                "/I",
                "imgui\\backends",
                "/I",
                "src",
                "/I",
                "src\\gui",
                "/I",
                "src\\audio",
                "/I",
                "src\\utils",
                "/I",
                "src\\config",
                "/I",
                "resources",
                "imgui\\imgui.cpp",
                "imgui\\imgui_draw.cpp",
                "imgui\\imgui_tables.cpp",
                "imgui\\imgui_widgets.cpp",
                "imgui\\backends\\imgui_impl_win32.cpp",
                "imgui\\backends\\imgui_impl_dx11.cpp",
                "src\\utils\\kissfft\\kiss_fft.c",
                "src\\utils\\kissfft\\kiss_fftr.c",
                "src\\audio\\AudioCapture.cpp",
                "src\\audio\\VUBuffer.cpp",
                "src\\audio\\FrequencyVUMeter.cpp",
                "src\\utils\\AppLauncher.cpp",
                "src\\utils\\ChannelLabels.cpp",
                "src\\utils\\fonts.cpp",
                "src\\utils\\Theme.cpp",
                "src\\utils\\SystemUtils.cpp",
                "src\\utils\\EqualizerAPOManager.cpp",
                "src\\utils\\PopupUtils.cpp",
                "src\\utils\\ButtonUtils.cpp",
                "src\\config\\ProfileManager.cpp",
                "src\\gui\\EQSwitchWindow.cpp",
                "src\\gui\\_partials\\ProfilesSelector.cpp",
                "src\\gui\\_partials\\Footer.cpp",
                "src\\gui\\_partials\\VUMetersSection.cpp",
                "src\\gui\\_partials\\VUMeters.cpp",
                "src\\gui\\_partials\\CheckSection.cpp",
                "src\\main.cpp",
                "build\\resource.res",
                "/link",
                "/SUBSYSTEM:WINDOWS",
                "dxgi.lib",
                "d3d11.lib",
                "user32.lib",
                "gdi32.lib",
                "Shell32.lib",
                "ole32.lib",
                "uuid.lib",
                "Advapi32.lib",
                "/OUT:bin\\EQSwitch.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        },
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ]
}